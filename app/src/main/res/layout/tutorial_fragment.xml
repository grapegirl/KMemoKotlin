<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="momo.kikiplus.refactoring.view.fragment.ui.TutorialFragment">

    <LinearLayout
        android:id="@+id/tutorial_back_color"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:background="#FFFFBB33"
            android:orientation="horizontal">

            <CheckBox
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:button="@drawable/checkbox_selector"
                android:clickable="false"
                android:visibility="invisible" />

            <CheckBox
                android:id="@+id/tutorial_main_checkbox1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_margin="1dp"
                android:button="@drawable/checkbox_selector"
                android:clickable="false" />

            <CheckBox
                android:id="@+id/tutorial_main_checkbox2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_margin="1dp"
                android:button="@drawable/checkbox_selector"
                android:clickable="false" />

            <CheckBox
                android:id="@+id/tutorial_main_checkbox3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_margin="1dp"
                android:button="@drawable/checkbox_selector"
                android:clickable="false" />

            <CheckBox
                android:id="@+id/tutorial_main_checkbox4"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_margin="1dp"
                android:button="@drawable/checkbox_selector"
                android:clickable="false" />

            <CheckBox
                android:id="@+id/tutorial_main_checkbox5"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_margin="1dp"
                android:button="@drawable/checkbox_selector"
                android:clickable="false" />

            <CheckBox
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_margin="1dp"
                android:layout_weight="1"
                android:button="@drawable/checkbox_selector"
                android:clickable="false"
                android:visibility="invisible" />
        </LinearLayout>

        <ViewFlipper
            android:id="@+id/tutorial_main_viewFlipper"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginBottom="20dp"
            android:layout_marginLeft="20dp"
            android:layout_marginRight="20dp"
            android:layout_marginTop="20dp"></ViewFlipper>

    </LinearLayout>


</FrameLayout>
